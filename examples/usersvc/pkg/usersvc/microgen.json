{
  "interface": "UserService",
  "generate": [
    {
      "name": "logging",
      "args": {
        "path": "service/logging.microgen.go",
        "easyjson": true,
        "took": true
      }
    },
    {
      "name": "logging",
      "args": {
        "path": "service/debug_logging.microgen.go",
        "name": "debugMiddleware",
        "easyjson": true,
        "inline": true
      }
    },
    {
      "name": "recovering",
      "args": {
        "path": "service/recovering.microgen.go",
        "stack": true
      }
    },
    {
      "name": "go-kit-transport",
      "args": {
        "exchanges": {
          "easyjson":true,
          "style":"camel"
        },
        "endpoints": {
          "latency": true
        },
        "client": {
          "trace": true
        },
        "server": {
          "trace": true
        }
      }
    },
    {
      "name":"go-kit-grpc",
      "args": {
        "protobuf":"github.com/devimteam/microgen/examples/usersvc/pb",
        "transportpkg": "transport",
        "client": {
          "trace":true
        }
      }
    }
  ]
}