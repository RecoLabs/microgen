{
  "interface": "UserService",
  "generate": [
    {
      "name": "logging",
      "args": {
        "path": "service/logging.go",
        "easyjson": true,
        "took": true
      }
    },
    {
      "name": "logging",
      "args": {
        "path": "service/debug_logging.go",
        "name": "debugMiddleware",
        "easyjson": true,
        "inline": true
      }
    },
    {
      "name": "recovering",
      "args": {
        "stack": true
      }
    },
    {
      "name": "go-kit-transport",
      "args": {
        "exchanges": {
          "easyjson":true,
          "style":"camel"
        },
        "endpoints": {
          "latency": true
        }
      }
    }
  ]
}